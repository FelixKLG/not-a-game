{
	"$schema": "https://biomejs.dev/schemas/1.6.4/schema.json",
	"organizeImports": {
	  "enabled": true
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"recommended": true
	  }
	},
	"formatter": {
	  "enabled": true,
	  "indentStyle": "space",
	  "indentWidth": 4
	},
	"javascript": {
	  "formatter": {
		"arrowParentheses": "asNeeded",
		"bracketSpacing": true,
		"bracketSameLine": true,
		"trailingComma": "all",
		"semicolons": "always"
	  }
	},
	"json": {
	  "formatter": {
		"enabled": true
	  }
	}
  }